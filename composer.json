{
  "name": "pollen-solutions/embed",
  "description": "Embed component",
  "homepage": "https://github.com/milkcreation/pollen-embed.git",
  "keywords": [
    "pollen-solutions",
    "components",
    "embed"
  ],
  "time": "2021-09-14",
  "license": "MIT",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy@presstify.com",
      "role": "Developper"
    }
  ],
  "require": {
    "php": ">=7.3",
    "composer/installers": "~1.0",
    "embed/embed": "^4.3",
    "vimeo/vimeo-api": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Pollen\\Embed\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pollen\\Embed\\Test\\": "tests"
    }
  },
  "suggest": {
    "essence/essence": "OEmbed library",
    "madcoda/php-youtube-api": "Youtube Embed Api library",
    "mpratt/embera": "OEmbed library"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
