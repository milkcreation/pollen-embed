{
  "name": "pollen-solutions/embed",
  "description": "Embed component",
  "homepage": "https://pollen-solutions/components/embed/",
  "keywords": [
    "pollen-solutions",
    "component",
    "embed"
  ],
  "time": "2020-12-12",
  "license": "MIT",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy.manner@milkcreation.fr",
      "role": "Developper"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.tigreblanc.fr/pollen-solutions/"
    }
  ],
  "require": {
    "php": ">=7.3",
    "composer/installers": "~1.0",
    "embed/embed": "^4.3",
    "presstify/framework": ">=1.4",
    "vimeo/vimeo-api": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Pollen\\Embed\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pollen\\Embed\\Test\\": "tests"
    }
  },
  "suggest": {
    "essence/essence": "OEmbed library",
    "madcoda/php-youtube-api": "Youtube Embed Api library",
    "mpratt/embera": "OEmbed library"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
